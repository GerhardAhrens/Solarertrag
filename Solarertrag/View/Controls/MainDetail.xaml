<UserControl
    x:Class="Solarertrag.View.Controls.MainDetail"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Solarertrag.View.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <TextBlock Text="Bearbeitung Details" />
        <Button
            Width="200"
            Height="40"
            Command="{Binding Path=CmdAgg[CloseDetailCommand]}"
            Content="Detail Dialog schließen" />

    </Grid>
</UserControl>
