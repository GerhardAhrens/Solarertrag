<Application
    x:Class="Solarertrag.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Solarertrag"
    StartupUri="View\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Style\StringResources.xaml" />
                <ResourceDictionary Source="Resources\Style\IconFontString.xaml" />
                <ResourceDictionary Source="Resources\Style\MainMenuStyle.xaml" />
                <ResourceDictionary Source="Resources\Style\ListViewStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/EasyPrototypingNET;component/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
