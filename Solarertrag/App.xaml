<Application
    x:Class="Solarertrag.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Solarertrag"
    StartupUri="View\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <!--  https://fontawesome.com/  -->
            <FontFamily x:Key="FontAW5BR">/Resources/Font/Font Awesome 6 Brands-Regular-400.otf#Font Awesome 6 Brands Regular</FontFamily>
            <FontFamily x:Key="FontAW5FR">/Resources/Font/Font Awesome 6 Free-Regular-400.otf#Font Awesome 6 Free Regular</FontFamily>
            <FontFamily x:Key="FontAW5Solid">/Resources/Font/Font Awesome 6 Free-Solid-900.otf#Font Awesome 6 Free Solid</FontFamily>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Style\IconFontString.xaml" />
                <ResourceDictionary Source="pack://application:,,,/EasyPrototypingNET;component/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
